[core]
	autocrlf = false
	editor = code-insiders --wait --new-window
[init]
	defaultBranch = main
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code-insiders --wait --diff $LOCAL $REMOTE
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = code-insiders --wait $MERGED
